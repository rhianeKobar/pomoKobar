{
	"manifest_version": 2,
	"name": "PomoKobar ",
	"description": "Quick website launcher",
	"version": "1.0.0",
	"icons": {"128":"pomoIcon128.png"},
	"browser_action":{
		"default_icon": "pomoIcon.png",
		"default_popup": "popup.html"
	},
	"permissions":["activeTab"],
	"background":{
		"scripts": ["popup.js"]
	},
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://kit.fontawesome.com/65db07c83e.js; object-src 'self'"
}